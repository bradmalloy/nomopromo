{
  "name": "nomopromo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Removes premieres from Youtube results since they're annoying.",
  "icons": {
    "128": "images/nobell_128x128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://*.youtube.com/*",
    "https://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "scripts/nomopromo.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
